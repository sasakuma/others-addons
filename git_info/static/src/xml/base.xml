<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="DashboardShare">
        <t t-jquery=".user-heading" t-operation="after">
            <t t-set="git_info" t-value="widget.data.git_info"/>
            <t t-if="git_info">
                <div class="git-info tab-content">
                    <h5>Git - Community Edition: <t t-esc="git_info"/></h5>
                </div>
            </t>
        </t>
    </t>
</templates>
